				"\t<div>",
					"\t\t<PageHeader :title=\"title\" :items=\"items\" />",
					"\t\t<div class=\"row\">",
						"\t\t\t<div class=\"col-lg-12\">",
							"\t\t\t\t",
						"\t\t\t</div>",
					"\t\t</div>",
				"\t</div>",
			"</template>",
			"",
			"<script>",
				"\texport default{",
					"\t\thead() {",
						"\t\t\treturn {",
							"\t\t\t\ttitle: `$${this.title} Dashboard | Minton - Nuxtjs Responsive Admin ","\t\t\t\tDashboard Template`,",
						"\t\t\t};",
					"\t\t},",
					"\t\tcomponents: {",
					"\t\t},",
					"\t\tdata() {",
						"\t\t\treturn {",
							"\t\t\t\ttitle: \"${1:Data}\",",
							"\t\t\t\titems: [",
