	"Template Initial Script Form - Minton": {
		"prefix": "initialScriptFormMinton",
		"body": [
		"<script>",
  			"\timport { required, email, minLength, sameAs, maxLength, minValue, maxValue, numeric, url, alphaNum } from 'vuelidate/lib/validators'",
			"",
			"\texport default{",
				"\t\tprops: ['modal_id'],",
				"\t\tdata() {",
				  "\t\t\treturn {",
					 "\t\t\t\t${1:typeform}: {",
						"\t\t\t\t\tname: ''",
					 "\t\t\t\t},",
					 "\t\t\t\t${2:typesubmit}: false,",
				  "\t\t\t}",
				"\t\t},",
				"\t\tvalidations: {",
				  "\t\t\t${1:typeform}: {",
						"\t\t\t\tname: {",
							 "\t\t\t\t\trequired",
						"\t\t\t\t}",
				  "\t\t\t},",
				"\t\t},",
				"\t\tmethods: {",
				  "\t\t\tsubmitForm(e) {",
					 "\t\t\t\tthis.${2:typesubmit} = true",
					 "\t\t\t\tthis.$$v.$$touch()",
					 "\t\t\t\tif(!this.$$v.$$invalid) {",
						"\t\t\t\t\tconsole.log('simpan')",
					 "\t\t\t\t}",
				  "\t\t\t},",
				  "\t\t\tcancelModal() {",
					 "\t\t\t\tthis.${2:typesubmit} = false",
					 "\t\t\t\tthis.${1:typeform}= {",
						"\t\t\t\t\tname: ''",
					 "\t\t\t\t},",
					 "\t\t\t\tthis.$$bvModal.hide(this.modal_id)",
				  "\t\t\t}",
				"\t\t}",
			"\t}",
		"</script>",
		""
		]
	},
	"Template Data Table - Minton": {
		"prefix": "cardTableMinton",
		"body": [
			"<template>",
				"\t<div class=\"card\">",
					"\t\t<div class=\"card-header border-bottom bg-transparent\">",
						"\t\t\t<div class=\"row\">",
							"\t\t\t\t<div class=\"col-sm-12 col-md-6\">",
								"\t\t\t\t\t<h5 class=\"header-title mb-0\">${1:Data List}</h5>",
							"\t\t\t\t</div>",
							"\t\t\t\t<div class=\"col-sm-12 col-md-6\">",
								"\t\t\t\t\t<div class=\"float-sm-right\">",
								"\t\t\t\t\t<button type=\"button\" class=\"btn btn-primary btn-sm\" v-b-modal.modalAddData>",
									"\t\t\t\t\t\t<i class=\"mdi mdi-plus-circle mr-1\"></i> Add Data",
								"\t\t\t\t\t</button>",
								"\t\t\t\t\t<!-- <FormAddData modal_id=\"modalAddData\"></FormAddData> -->",
								"\t\t\t\t\t</div>",
							"\t\t\t\t</div>",
						"\t\t\t</div>",
					"\t\t</div>",
					"\t\t<div class=\"card-body\">",
						"\t\t\t<div class=\"row mb-md-2\">",
							"\t\t\t\t<div class=\"col-sm-12 col-md-6\">",
								"\t\t\t\t\t<div id=\"\" class=\"dataTables_length\">",
									"\t\t\t\t\t\t<label class=\"d-inline-flex align-items-center\">",
										"\t\t\t\t\t\t\tShow&nbsp;",
										"\t\t\t\t\t\t\t<b-form-select v-model=\"perPage\" size=\"sm\" :options=\"pageOptions\"></b-form-select>",
										"\t\t\t\t\t\t\t<!-- &nbsp;entries -->",
									"\t\t\t\t\t\t</label>",
								"\t\t\t\t\t</div>",
							"\t\t\t\t</div>",
							"\t\t\t\t<div class=\"col-sm-12 col-md-6\">",
								"\t\t\t\t\t<div id=\"\" class=\"dataTables_filter text-md-right\">",
									"\t\t\t\t\t\t<label class=\"d-inline-flex align-items-center\">",
										"\t\t\t\t\t\t\tSearch:",
										"\t\t\t\t\t\t\t<b-form-input v-model=\"filter\" type=\"search\" placeholder=\"Search...\" class=\"form-control form-control-sm ml-2\"></b-form-input>",
									"\t\t\t\t\t\t</label>",
								"\t\t\t\t\t</div>",
							"\t\t\t\t</div>",
						"\t\t\t</div>",
						"\t\t\t<div class=\"table-responsive table-striped table-sm mb-0\">",
							"\t\t\t\t<b-table",
								"\t\t\t\t\t:items=\"tableData\"",
								"\t\t\t\t\t:fields=\"fields\"",
								"\t\t\t\t\tresponsive=\"sm\"",
								"\t\t\t\t\t:per-page=\"perPage\"",
								"\t\t\t\t\t:current-page=\"currentPage\"",
								"\t\t\t\t\t:sort-by.sync=\"sortBy\"",
								"\t\t\t\t\t:sort-desc.sync=\"sortDesc\"",
								"\t\t\t\t\t:filter=\"filter\"",
								"\t\t\t\t\t:filter-included-fields=\"filterOn\"",
								"\t\t\t\t\tshow-empty",
								"\t\t\t\t\t@filtered=\"onFiltered\">",
								"\t\t\t\t\t<template v-slot:cell(action)=\"data\">",
									"\t\t\t\t\t\t<div class=\"text-center\">",
										"\t\t\t\t\t\t\t<button class=\"btn btn-xs btn-primary\" title=\"Show\"><i class=\"mdi mdi-eye\"></i></button>",
										"\t\t\t\t\t\t\t<button class=\"btn btn-xs btn-success\" title=\"Edit\"><i class=\"mdi mdi-square-edit-outline\"></i></button>",
										"\t\t\t\t\t\t\t<button class=\"btn btn-xs btn-danger\" title=\"Delete\" @click=\"deleteConfirmation(data.index)\"><i class=\"mdi mdi-delete\"></i></button>",
									"\t\t\t\t\t\t</div>",
								"\t\t\t\t\t</template>",
							"\t\t\t\t</b-table>",
						"\t\t\t</div>",
						"\t\t\t<div class=\"row\">",
							"\t\t\t\t<div class=\"col\">",
								"\t\t\t\t\t<div class=\"paging_simple_numbers float-right\">",
									"\t\t\t\t\t\t<ul class=\"pagination pagination-rounded mb-0\">",
										"\t\t\t\t\t\t\t<b-pagination v-model=\"currentPage\" :total-rows=\"totalRows\" :per-page=\"perPage\"></b-pagination>",
									"\t\t\t\t\t\t</ul>",
								"\t\t\t\t\t</div>",
							"\t\t\t\t</div>",
						"\t\t\t</div>",
					"\t\t</div>",
				"\t</div>",
			"</template>",
			"",
			"<script>",
				"\timport { tableData } from '@/api/data'",
				"\timport Swal from \"sweetalert2\";",

				"\texport default{",
					"\t\tcomponents: {",
					"\t\t},",
					"\t\tdata() {",
						"\t\t\treturn {",
							"\t\t\t\ttableData: tableData,",
							"\t\t\t\ttotalRows: 1,",
							"\t\t\t\tcurrentPage: 1,",
							"\t\t\t\tperPage: 10,",
							"\t\t\t\tpageOptions: [10, 25, 50, 100],",
							"\t\t\t\tfilter: null,",
							"\t\t\t\tfilterOn: [],",
							"\t\t\t\tsortBy: 'name',",
							"\t\t\t\tsortDesc: false,",
							"\t\t\t\tfields: [",
									"\t\t\t\t\t\t{",
										"\t\t\t\t\t\t\tkey: 'name',",
										"\t\t\t\t\t\t\tsortable: true",
									"\t\t\t\t\t\t},",
									"\t\t\t\t\t\t{",
										"\t\t\t\t\t\t\tkey: 'position',",
										"\t\t\t\t\t\t\tsortable: false",
									"\t\t\t\t\t\t},",
									"\t\t\t\t\t\t{",
										"\t\t\t\t\t\t\tkey: 'office',",
										"\t\t\t\t\t\t\tsortable: false",
									"\t\t\t\t\t\t},",
									"\t\t\t\t\t\t{",
										"\t\t\t\t\t\t\tkey: 'age',",
										"\t\t\t\t\t\t\tsortable: true",
									"\t\t\t\t\t\t},",
									"\t\t\t\t\t\t{",
										"\t\t\t\t\t\t\tkey: 'date',",
										"\t\t\t\t\t\t\tsortable: true",
									"\t\t\t\t\t\t},",
									"\t\t\t\t\t\t{",
										"\t\t\t\t\t\t\tkey: 'salary',",
										"\t\t\t\t\t\t\tlabel: 'Bayaran',",
										"\t\t\t\t\t\t\tsortable: true",
									"\t\t\t\t\t\t},",
									"\t\t\t\t\t\t{",
										"\t\t\t\t\t\t\tkey: 'action',",
										"\t\t\t\t\t\t\tlabel: 'Action',",
										"\t\t\t\t\t\t\tthClass: 'text-center',",
									"\t\t\t\t\t\t},",
							"\t\t\t\t],",
						"\t\t\t}",
					"\t\t},",
					"\t\tcomputed: {",
						"\t\t\trows() {",
							"\t\t\t\treturn this.tableData.length",
						"\t\t\t},",
					"\t\t},",
					"\t\tmounted() {",
						"\t\t\tthis.totalRows = this.rows",
					"\t\t},",
					"\t\tmethods: {",
						"\t\t\tonFiltered(filteredItems) {",
							"\t\t\t\tthis.totalRows = filteredItems.length",
							"\t\t\t\tthis.currentPage = 1",
						"\t\t\t},",
						"\t\t\tdeleteConfirmation(id){",
							"\t\t\t\tSwal.fire({",
								"\t\t\t\t\ttitle: \"Are you sure?\",",
								"\t\t\t\t\ttext: \"You won't be able to revert this!\",",
								"\t\t\t\t\ticon: \"warning\",",
								"\t\t\t\t\tshowCancelButton: true,",
								"\t\t\t\t\tconfirmButtonColor: \"#3085d6\",",
								"\t\t\t\t\tcancelButtonColor: \"#d33\",",
								"\t\t\t\t\tconfirmButtonText: \"Delete\",",
							"\t\t\t\t}).then((result) => {",
								"\t\t\t\t\tif (result.value) {",
									"\t\t\t\t\t\tSwal.fire({",
										"\t\t\t\t\t\t\ttitle: \"Deleted!\",",
										"\t\t\t\t\t\t\ttext: \"Your file has been deleted.\",",
										"\t\t\t\t\t\t\ticon: \"success\",",
										"\t\t\t\t\t\t\tshowConfirmButton: false,",
										"\t\t\t\t\t\t\ttimer: 1000",
									"\t\t\t\t\t\t})",
								"\t\t\t\t\t}",
							"\t\t\t\t});",
						"\t\t\t}",
					"\t\t}",
				"\t}",
			"</script>"
		]
	},
	"Template Modal Form - MInton": {
		"prefix": "modalFormMinton",
		"body": [
			"<template>",
				"\t<div>",
					"\t\t<b-modal",
						"\t\t\t:id=\"modal_id\"",
						"\t\t\ttitle=\"${3:Form Tambah Data}\"",
						"\t\t\ttitle-class=\"font-18\"",
						"\t\t\tbody-class=\"p-4\"",
						"\t\t\thide-footer",
						"\t\t\t@hide=\"cancelModal\"",
					"\t\t>",
						"\t\t\t<form action=\"#\" @submit.prevent=\"submitForm\">",
							"\t\t\t\t<div class=\"form-group\">",
								"\t\t\t\t\t<label>Required</label>",
								"\t\t\t\t\t<input v-model=\"${1:typeform}.name\" type=\"text\" class=\"form-control\" placeholder=\"Type something\" name=\"name\" :class=\"{ 'is-invalid': ${2:typesubmit} && $$v.${1:typeform}.name.$$error }\" />",
								"\t\t\t\t\t<div v-if=\"${2:typesubmit} && $$v.${1:typeform}.name.$$error\" class=\"invalid-feedback\">",
									"\t\t\t\t\t\t<span v-if=\"!$$v.${1:typeform}.name.required\">This value is required.</span>",
								"\t\t\t\t\t</div>",
							"\t\t\t\t</div>",
							"\t\t\t\t<div class=\"row\">",
								"\t\t\t\t\t<div class=\"col-12 text-center\">",
									"\t\t\t\t\t\t<div class=\"form-group mb-0\">",
										"\t\t\t\t\t\t\t<button type=\"submit\" class=\"btn btn-primary btn-sm\">Submit</button>",
										"\t\t\t\t\t\t\t<button type=\"reset\" class=\"btn btn-secondary btn-sm m-l-5 ml-1\" @click=\"cancelModal\">Cancel</button>",
									"\t\t\t\t\t\t</div>",
								"\t\t\t\t\t</div>",
							"\t\t\t\t</div>",
						"\t\t\t</form>",
					"\t\t</b-modal>",
				"\t</div>",
